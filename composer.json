{
    "name": "ajtarragona/t-batches",
    "description": "Queued monitorable batch jobs for Laravel",
    "keywords": ["laravel", "queues", "jobs","batch"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Txomin Medrano Martorell",
            "email": "tmedrano@tarragona.cat"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "^6.0|^7.0",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "Ajtarragona\\TBatches\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ajtarragona\\TBatches\\TBatchesServiceProvider"
            ],
            "aliases": {
                "TBatches" : "Ajtarragona\\TBatches\\Facades\\TBatchesFacade"
            }
        }
    }
}
